#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/ciami/blog/shokax-can/node_modules/.pnpm/hexo-cli@4.3.2/node_modules/hexo-cli/bin/node_modules:/home/ciami/blog/shokax-can/node_modules/.pnpm/hexo-cli@4.3.2/node_modules/hexo-cli/node_modules:/home/ciami/blog/shokax-can/node_modules/.pnpm/hexo-cli@4.3.2/node_modules:/home/ciami/blog/shokax-can/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/ciami/blog/shokax-can/node_modules/.pnpm/hexo-cli@4.3.2/node_modules/hexo-cli/bin/node_modules:/home/ciami/blog/shokax-can/node_modules/.pnpm/hexo-cli@4.3.2/node_modules/hexo-cli/node_modules:/home/ciami/blog/shokax-can/node_modules/.pnpm/hexo-cli@4.3.2/node_modules:/home/ciami/blog/shokax-can/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../hexo-cli/bin/hexo" "$@"
else
  exec node  "$basedir/../hexo-cli/bin/hexo" "$@"
fi
