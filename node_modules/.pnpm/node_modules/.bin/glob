#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/ciami/blog/shokax-can/node_modules/.pnpm/glob@11.0.3/node_modules/glob/dist/esm/node_modules:/home/ciami/blog/shokax-can/node_modules/.pnpm/glob@11.0.3/node_modules/glob/dist/node_modules:/home/ciami/blog/shokax-can/node_modules/.pnpm/glob@11.0.3/node_modules/glob/node_modules:/home/ciami/blog/shokax-can/node_modules/.pnpm/glob@11.0.3/node_modules:/home/ciami/blog/shokax-can/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/ciami/blog/shokax-can/node_modules/.pnpm/glob@11.0.3/node_modules/glob/dist/esm/node_modules:/home/ciami/blog/shokax-can/node_modules/.pnpm/glob@11.0.3/node_modules/glob/dist/node_modules:/home/ciami/blog/shokax-can/node_modules/.pnpm/glob@11.0.3/node_modules/glob/node_modules:/home/ciami/blog/shokax-can/node_modules/.pnpm/glob@11.0.3/node_modules:/home/ciami/blog/shokax-can/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../glob/dist/esm/bin.mjs" "$@"
else
  exec node  "$basedir/../glob/dist/esm/bin.mjs" "$@"
fi
