#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/ciami/blog/shokax-can/node_modules/.pnpm/esbuild@0.25.8/node_modules/esbuild/bin/node_modules:/home/ciami/blog/shokax-can/node_modules/.pnpm/esbuild@0.25.8/node_modules/esbuild/node_modules:/home/ciami/blog/shokax-can/node_modules/.pnpm/esbuild@0.25.8/node_modules:/home/ciami/blog/shokax-can/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/ciami/blog/shokax-can/node_modules/.pnpm/esbuild@0.25.8/node_modules/esbuild/bin/node_modules:/home/ciami/blog/shokax-can/node_modules/.pnpm/esbuild@0.25.8/node_modules/esbuild/node_modules:/home/ciami/blog/shokax-can/node_modules/.pnpm/esbuild@0.25.8/node_modules:/home/ciami/blog/shokax-can/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
"$basedir/../.pnpm/esbuild@0.25.8/node_modules/esbuild/bin/esbuild"   "$@"
exit $?
